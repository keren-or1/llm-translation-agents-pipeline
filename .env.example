# Translation Agents Pipeline - Environment Configuration
# Copy this file to .env and modify values as needed

# Embedding Model Configuration
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Cache Configuration
CACHE_DIR=.cache
ENABLE_CACHE=true

# Input/Output Files
INPUT_FILE=docs/experiments_input.json
OUTPUT_FILE=docs/experiment_results.json
MARKDOWN_OUTPUT=docs/experiment_data.md
GRAPH_OUTPUT=screenshots/translation_distance_graph.png

# Visualization Settings
GRAPH_DPI=300
GRAPH_WIDTH=12
GRAPH_HEIGHT=6

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/translation_agents.log

# Performance Settings
BATCH_SIZE=1
PARALLEL_PROCESSING=false

# Development
DEBUG_MODE=false
VERBOSE=false

# Note: All values have sensible defaults in config.py
# Only override here if you need different settings
